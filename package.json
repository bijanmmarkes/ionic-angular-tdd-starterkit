{
    "name": "App",
    "version": "0.0.1",
    "author": "Ionic Framework",
    "homepage": "http://ionicframework.com/",
    "private": true,
    "main": "main.js",
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "test": "karma start ./test-config/karma.conf.js",
        "test-ci": "karma start ./test-config/karma.conf.js --single-run",
        "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
        "e2e": "npm run e2e-update && npm run e2e-test",
        "e2e-test": "protractor ./test-config/protractor.conf.js",
        "e2e-update": "webdriver-manager update --standalone false --gecko false",
        "lint": "ionic-app-scripts lint",
        "prod": "ionic-app-scripts build --prod --release",
        "android:run": "ionic cordova run android",
        "android:key": "keytool -genkey -v -keystore android_key.keystore -storepass testestore -alias testealias -keypass testepass -keyalg RSA -keysize 2048 -validity 3650",
        "android:release": "ionic cordova build android --prod --release -- -- --keystore=android_key.keystore --storePassword=testestore --alias=testealias --password=testepass",
        "electron": "electron .",
        "build:osx": "build --mac --x64",
        "build:win": "build --windows --ia32"
    },
    "dependencies": {
        "@angular/animations": "5.2.11",
        "@angular/common": "5.2.11",
        "@angular/compiler": "5.2.11",
        "@angular/compiler-cli": "5.2.11",
        "@angular/core": "5.2.11",
        "@angular/forms": "5.2.11",
        "@angular/http": "5.2.11",
        "@angular/platform-browser": "5.2.11",
        "@angular/platform-browser-dynamic": "5.2.11",
        "@angular/router": "^6.0.7",
        "@auth0/cordova": "^0.3.0",
        "@ionic-native/core": "4.8.0",
        "@ionic-native/deeplinks": "^4.9.0",
        "@ionic-native/network": "^4.9.0",
        "@ionic-native/splash-screen": "4.8.0",
        "@ionic-native/status-bar": "4.8.0",
        "@okta/okta-auth-js": "^1.17.0",
        "@saphocom/auth0-plugin-cordova": "^0.2.1",
        "angular-oauth2-oidc": "^4.0.2",
        "angular-storage": "0.0.15",
        "auth0-js": "^9.6.1",
        "auth0-lock": "^11.7.2",
        "cordova-browser": "^4.1.0",
        "cordova-ios": "^4.5.4",
        "cordova-plugin-customurlscheme": "^4.3.0",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-inappbrowser": "~1.7.1",
        "cordova-plugin-ionic-keyboard": "^2.0.5",
        "cordova-plugin-ionic-webview": "^1.1.19",
        "cordova-plugin-network-information": "^2.0.1",
        "cordova-plugin-safariviewcontroller": "^1.5.4",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "express": "^4.16.3",
        "ionic-angular": "3.9.2",
        "ionicons": "3.0.0",
        "npm": "^6.2.0",
        "rxjs": "5.5.11",
        "sw-toolbox": "3.6.0",
        "typescript": "^2.9.2",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular/cli": "^1.4.8",
        "@ionic/app-scripts": "^3.1.10",
        "@ionic/storage": "^2.1.3",
        "@types/jasmine": "^2.8.8",
        "@types/node": "^8.10.20",
        "angular2-template-loader": "^0.6.2",
        "electron": "^2.0.3",
        "electron-builder": "^19.47.1",
        "html-loader": "^0.5.5",
        "ionic-mocks": "^1.3.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jasmine": "^2.99.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^1.3.0",
        "karma-jasmine": "^1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.13",
        "null-loader": "^0.1.1",
        "protractor": "^5.3.2",
        "ts-loader": "^3.5.0",
        "ts-node": "^3.3.0"
    },
    "description": "An Ionic project",
    "cordova": {
        "plugins": {
            "cordova-plugin-network-information": {},
            "cordova-plugin-safariviewcontroller": {},
            "cordova-plugin-customurlscheme": {
                "URL_SCHEME": "com.myApp",
                "ANDROID_SCHEME": "com.myApp",
                "ANDROID_HOST": "myApp.auth0.com",
                "ANDROID_PATHPREFIX": "/cordova/com.myApp/callback"
            },
            "cordova-plugin-device": {},
            "cordova-plugin-ionic-keyboard": {},
            "cordova-plugin-ionic-webview": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "browser",
            "ios"
        ]
    }
}
